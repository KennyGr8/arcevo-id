{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./backend",
    "paths": {
      "@/*": ["auth-kit-core/src/*"],
      "@types/*": ["auth-kit-core/src/@types/*"],
      "@common/*": ["auth-kit-core/src/common/*"],
      "@utils/*": ["auth-kit-core/src/common/utils/*"],
      "@strategies/*": ["auth-kit-core/src/common/strategies/*"],
      "@validators/*": ["auth-kit-core/src/common/validators/*"],
      "@constants/*": ["auth-kit-core/src/common/constants/*"],
      "@configs/*": ["auth-kit-core/src/common/configs/*"],
      "@queues/*": ["auth-kit-core/src/common/queues/*"],
      "@database/*": ["auth-kit-core/src/database/*"],
      "@generated/*": ["auth-kit-core/src/generated/*"],
      "@modules/*": ["auth-kit-core/src/modules/*"],
      "@middleware/*": ["auth-kit-core/src/middleware/*"],
      "@mailers/*": ["auth-kit-core/src/mailers/*"],
      "@core/*": ["auth-kit-core/src/core/*"],
      "@interfaces/*": ["auth-kit-core/src/core/interfaces/*"],
      "@adapters/*": ["auth-kit-core/src/core/adapters/*"],
      "@contracts/*": ["auth-kit-core/src/core/contracts*"],
      "@prisma/*": ["adapters/database/prisma/*"],
      "@scripts/*": ["scripts/*"]
    }
  }
}
